    "build": "tsup src/index.ts --dts --clean --format cjs,esm --out-dir dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sendaifun/solana-agent-kit.git"
  },
  "homepage": "https://github.com/sendaifun/solana-agent-kit/tree/v2/packages/core",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "keywords": ["solana", "agent", "ai", "solana agent kit", "sendai"],
  "author": "sendaifun",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.15.3",
  "dependencies": {
    "@langchain/core": "^0.3.44",
    "@solana/web3.js": "^1.98.0",
    "ai": "^4.1.5",
    "bs58": "^6.0.0",
    "rpc-websockets": "^10.0.0",
    "tweetnacl": "^1.0.3",
    "zod": "^3.24.1"
  }
